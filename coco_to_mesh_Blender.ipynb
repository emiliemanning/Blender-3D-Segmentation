        "# @title Create mesh\n",
        "# @title Mesh for Blender\n",
        "import numpy as np\n",
        "import cv2\n",
        "import os\n",
        "\n",
        "default_sampling_step = 1\n",
        "default_x_range_mm = x_range_mm\n",
        "default_y_range_mm = x_range_mm\n",
        "default_thickness_mm = 0.002\n",
        "\n",
        "def export_single_slice_to_ply(fp, image_path, output_path,\n",
        "                                sampling_step=None,\n",
        "                                x_range_mm=None,\n",
        "                                y_range_mm=None,\n",
        "                                thickness_mm=None):\n",
        "\n",
        "    if sampling_step is None:\n",
        "        sampling_step = default_sampling_step\n",
        "    if x_range_mm is None:\n",
        "        x_range_mm = default_x_range_mm\n",
        "    if y_range_mm is None:\n",
        "        y_range_mm = default_y_range_mm\n",
        "    if thickness_mm is None:\n",
        "        thickness_mm = default_thickness_mm\n",
        "    image = cv2.imread(image_path)\n",
        "    if image is None:\n",
        "        print(f\"Error: could not load image at {image_path}\")\n",
        "        return\n",
        "\n",
        "    h, w = image.shape[:2]\n",
        "    # uv\n",
        "    uu, vv = np.meshgrid(\n",
        "        np.arange(0, w, sampling_step),\n",
        "        np.arange(0, h, sampling_step)\n",
        "    )\n",
        "    uv_coords = np.stack([uu.ravel(), vv.ravel()], axis=-1)\n",
        "    xyz_coords = fp.get_xyz_from_uv(uv_coords)\n",
        "    #flip z\n",
        "    xyz_coords[:, 2] *= -1\n",
        "    #color for each point at image\n",
        "    colors = image[uv_coords[:, 1].astype(int), uv_coords[:, 0].astype(int)]\n",
        "\n",
        "    x_min, x_max = x_range_mm\n",
        "    y_min, y_max = y_range_mm\n",
        "\n",
        "    mask = (\n",
        "        (xyz_coords[:, 0] >= x_min) & (xyz_coords[:, 0] <= x_max) &\n",
        "        (xyz_coords[:, 1] >= y_min) & (xyz_coords[:, 1] <= y_max)\n",
        "    )\n",
        "\n",
        "    if not np.any(mask):\n",
        "        print(\"No points within bounding box\")\n",
        "        return\n",
        "\n",
        "    uv_coords = uv_coords[mask]\n",
        "    xyz_coords = xyz_coords[mask]\n",
        "    colors = colors[mask]\n",
        "\n",
        "    #center the mesh for blender\n",
        "    x_center = (x_min + x_max) / 2\n",
        "    y_center = (y_min + y_max) / 2\n",
        "    xyz_coords[:, 0] -= x_center\n",
        "    xyz_coords[:, 1] -= y_center\n",
        "\n",
        "    # remapping valid indics in 2D grid\n",
        "    grid_mask = mask.reshape(vv.shape)\n",
        "    valid_map = -np.ones_like(grid_mask, dtype=int)\n",
        "    valid_indices = np.flatnonzero(mask)\n",
        "    valid_map[np.where(grid_mask)] = np.arange(len(xyz_coords))\n",
        "\n",
        "    faces = []\n",
        "    for y in range(grid_mask.shape[0] - 1):\n",
        "        for x in range(grid_mask.shape[1] - 1):\n",
        "            if not (grid_mask[y, x] and grid_mask[y, x + 1] and grid_mask[y + 1, x] and grid_mask[y + 1, x + 1]):\n",
        "                continue\n",
        "            v0 = valid_map[y, x]\n",
        "            v1 = valid_map[y, x + 1]\n",
        "            v2 = valid_map[y + 1, x]\n",
        "            v3 = valid_map[y + 1, x + 1]\n",
        "            faces.append([v0, v1, v2])\n",
        "            faces.append([v1, v3, v2])\n",
        "\n",
        "    #  thickness of each slice\n",
        "    offset = np.array([0, 0, -thickness_mm])\n",
        "    verts_top = xyz_coords\n",
        "    verts_bottom = xyz_coords + offset\n",
        "    verts = np.vstack([verts_top, verts_bottom])\n",
        "    colors = np.vstack([colors, colors])\n",
        "    verts *= 16.0 # scales everything by 16 to match blender units\n",
        "\n",
        "\n",
        "    # Add side faaces to meshes (fill in gaps)\n",
        "    thick_faces = []\n",
        "    for f in faces:\n",
        "        a, b, c = f\n",
        "        a_b, b_b, c_b = a + len(verts_top), b + len(verts_top), c + len(verts_top)\n",
        "        thick_faces.append([a, b, c])  # top\n",
        "        thick_faces.append([c_b, b_b, a_b])  # bottom (flip )\n",
        "        # sides\n",
        "        thick_faces.extend([[a, a_b, b], [b, a_b, b_b]])\n",
        "        thick_faces.extend([[b, b_b, c], [c, b_b, c_b]])\n",
        "        thick_faces.extend([[c, c_b, a], [a, c_b, a_b]])\n",
        "\n",
        "\n",
        "    with open(output_path, 'w') as f:\n",
        "        f.write(\"ply\\n\")\n",
        "        f.write(\"format ascii 1.0\\n\")\n",
        "        f.write(f\"element vertex {len(verts)}\\n\")\n",
        "        f.write(\"property float x\\nproperty float y\\nproperty float z\\n\")\n",
        "        f.write(\"property uchar red\\nproperty uchar green\\nproperty uchar blue\\n\")\n",
        "        f.write(f\"element face {len(thick_faces)}\\n\")\n",
        "        f.write(\"property list uchar int vertex_indices\\n\")\n",
        "        f.write(\"end_header\\n\")\n",
        "\n",
        "        for v, c in zip(verts, colors):\n",
        "            f.write(f\"{v[0]} {v[1]} {v[2]} {c[2]} {c[1]} {c[0]}\\n\")\n",
        "        for face in thick_faces:\n",
        "            f.write(f\"3 {' '.join(map(str, face))}\\n\")\n",
        "\n",
        "    print(f\"PLY mesh exported to {output_path}\")\n",
        "\n",
        "if create_meshes:\n",
        "  #loop through all sections and export each mesh\n",
        "  for i, (fp, plane_input) in enumerate(zip(fps, plane_inputs)):\n",
        "      image_path = plane_input[\"image_file_path\"]\n",
        "      # output_path = f\"slice_{i:03d}.ply\"\n",
        "      image_name = os.path.basename(image_path)\n",
        "      output_path = os.path.splitext(image_name)[0] + \"_mesh.ply\"\n",
        "\n",
        "      export_single_slice_to_ply(fp, image_path, output_path)\n",
        "\n"
      ]
    }
